<Window x:Class="SelezionaImmagine.SceltaEffettuata"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SelezionaImmagine"
        mc:Ignorable="d"
        Title="SceltaEffettuata" Height="200" Width="300">
    <Grid>
        <Label Content="Hai scelto " HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top"/>
        <Label x:Name="CittàScelta" Content="Label" HorizontalAlignment="Left" Margin="116,30,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18" Foreground="#FFFF0C0C"/>
        <Image x:Name="ImgScelta" HorizontalAlignment="Left" Height="76" Margin="65,69,0,0" VerticalAlignment="Top" Width="155"/>
        <Button x:Name="Indietro" Content="Indietro" HorizontalAlignment="Left" Margin="209,15,0,0" VerticalAlignment="Top" Width="75" Click="Indietro_Click"/>

    </Grid>
</Window>
